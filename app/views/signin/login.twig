<body>
	
	<div class="limiter">
		<div class="container-login100" style="background-image: url('public/img/login.png');">
			<div class="wrap-login100 p-t-30 p-b-50">
				<span class="login100-form-title p-b-41">
					<span class="tessellate-colors"> 
                    <span class="tessellate-icon">&#xf003;</span> Tessellate
                    </span>
				</span>
				<form class="login100-form validate-form p-b-33 p-t-5" action="login/new" method="POST">

					<div class="wrap-input100 validate-input" data-validate = "Enter username">
						<input class="input100" type="text" name="username" placeholder="User name">
						<span class="focus-input100" style="margin-top: -7px" data-placeholder="&#xe82a;"></span>
					</div>

                    <div class="wrap-input100 validate-input" data-validate = "Enter email">
						<input class="input100" type="email" name="email" placeholder="Email">
						<span class="fa focus-inputEmail" style="margin-top: -7px"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Enter password">
						<input class="input100" type="password" name="pass" placeholder="Password">
						<span class="focus-input100" style="margin-top: -7px" data-placeholder="&#xe80f;"></span>
					</div>

					<div class="container-login100-form-btn m-t-32">
						<button class="login100-form-btn">
							Login
						</button>
                        <br>
					</div>
					<div class="dislog d-flex justify-content-center">
						<a class="btn btn-light mt-2" style="margin: 0 auto" href="/">
							<i class="fa fa-arrow-left"></i> Retornar
						</a>
					</div>
                   <div id="my-signin2" style="margin: 10px"></div> 
				</form>
			</div>
		</div>
	</div>
    
 
	
	<script src="public/js/jquery-3.4.0.min.js"></script>
<!--===============================================================================================-->
	<script src="public/js/select2/select2.min.js"></script>
<!--===============================================================================================-->
	<script src="public/js/daterangepicker/moment.min.js"></script>
	<script src="public/js/daterangepicker/daterangepicker.js"></script>
<!--===============================================================================================-->
	<script src="public/js/countdowntime/countdowntime.js"></script>
<!--===============================================================================================-->
	<script src="public/js/loginmain.js"></script>
